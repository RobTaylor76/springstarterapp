<?xml version="1.0" encoding="UTF-8"?>

<databaseChangeLog xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
         http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.1.xsd">



	<changeSet id="1" author="rob">
		<createTable tableName="user">
			<column name="id" type="int">
				<constraints primaryKey="true" nullable="false" />
			</column>
			<column name="email" type="varchar(50)">
				<constraints nullable="false" />
			</column>
		</createTable>
		<addAutoIncrement catalogName="cat" columnDataType="int"
			columnName="id" incrementBy="1" startWith="100" tableName="user" />
			
		<sql 
            splitStatements="true"
            stripComments="true">
            insert into user (email) values ('r@b.com')
            insert into user (email) values ('b@b.com')
    </sql>
			
	</changeSet>


</databaseChangeLog>